{
    "name": "scottlaurent/fsrs",
    "description": "Free Spaced Repetition Scheduler (FSRS) Algorithm Implementation for PHP",
    "license": "MIT",
    "keywords": ["fsrs", "spaced repetition", "memory", "learning", "scheduler"],
    "authors": [
        {
            "name": "Scott Laurent",
            "email": "scott@baselineapplications.com"
        }
    ],
    "repositories": [],
    "require": {
        "php": "^8.1|^8.2|^8.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0|^11.0",
        "mockery/mockery": "^1.6.0",
        "fakerphp/faker": "^1.23"
    },
    "config": {
        "allow-plugins": {
            "phpunit/phpunit": true
        },
        "sort-packages": true,
        "optimize-autoloader": true,
        "preferred-install": "dist"
    },
    "autoload": {
        "psr-4": {
            "Scottlaurent\\FSRS\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "XDEBUG_MODE=coverage vendor/bin/phpunit",
        "test:coverage": "XDEBUG_MODE=coverage vendor/bin/phpunit --coverage-html coverage"
    }
}